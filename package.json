{
  "name": "mine_calc",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cross-env BROWSER=none vite",
    "build": "vite build && npm run del_docs && npm run rename_build_to_docs && npm run rename_index_to_404",
    "del_docs": "node -e \"require('fs').rmSync('docs', { recursive: true, force: true })\"",
    "rename_build_to_docs": "node -e \"require('fs').renameSync('build', 'docs')\"",
    "rename_index_to_404": "node -e \"require('fs').renameSync('docs/index.html', 'docs/404.html')\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.8.2",
    "@supabase/supabase-js": "2.89.0",
    "@types/node": "22.13.0",
